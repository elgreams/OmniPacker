{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    "fs:allow-write-text-file",
    "shell:default",
    "shell:allow-execute",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "binaries/DepotDownloader",
          "sidecar": true
        },
        {
          "name": "binaries/7zz",
          "sidecar": true
        }
      ]
    }
  ]
}
